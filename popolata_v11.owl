<owl:Ontology rdf:about="http://www.semanticweb.org/niki/ontologies/2024/11/merged">
  <x_1.1:creator xml:lang="ida.liu.se">Almut Herzog, almhe</x_1.1:creator>
  <x_1.1:publisher rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Dept. of Computer and Information Science, Linköpings universitet, Sweden</x_1.1:publisher>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">An ontology that describes security technologies as a taxonomy and as countermeasures to protection goals. Typical starting points for browsing should be the classes Asset, Countermeasure, Threat.</rdfs:comment>
  <rdfs:seeAlso rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://www.ida.liu.se/divisions/adit/security/projects/secont/SecurityLiterature.owl to view the literature(direct import)</rdfs:seeAlso>
  <owl:versionInfo rdf:datatype="http://www.w3.org/2001/XMLSchema#string">$Id: AHz-06.7-2.5.owl,v 1.6 2006/05/30 10:51:32 almhe Exp almhe $</owl:versionInfo>
</owl:Ontology>

<owl:ObjectProperty rdf:about="#affects">
  <owl:inverseOf rdf:resource="#isAffectedBy"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#isAffectedBy"/>

<owl:ObjectProperty rdf:about="#composedOf">
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Esprime quali risorse compongono la rete.</rdfs:comment>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#composes"/>

<owl:ObjectProperty rdf:about="#connects">
  <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#SymmetricProperty"/>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Esprime le interconnessioni interne ad un nodo o tra più nodi.</rdfs:comment>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#containsControl">
  <owl:inverseOf rdf:resource="#isContainedIn"/>
  <rdfs:domain rdf:resource="#Product"/>
  <rdfs:range rdf:resource="#SecurityMechanism"/>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A product can contain one or more countermeasure. For example: MS Word contains encryption and access control.</rdfs:comment>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#isContainedIn">
  <rdfs:domain rdf:resource="#SecurityMechanism"/>
  <rdfs:range rdf:resource="#Product"/>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A countermeasure is contained in one or more products. For example: Encryption part of MS Word.</rdfs:comment>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#containsTechnology">
  <owl:inverseOf rdf:resource="#residesOn"/>
  <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
  <rdfs:domain rdf:resource="#Technology"/>
  <rdfs:range rdf:resource="#Technology"/>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">One technolgoy asset can contain further technology assets. For example: A network contains hosts, hosts contain data.</rdfs:comment>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#residesOn">
  <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
  <rdfs:domain rdf:resource="#Technology"/>
  <rdfs:range rdf:resource="#Technology"/>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">One technology asset resides on one or more technology assets. For example: Data resides on Host, Host resides on Network.</rdfs:comment>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#credentialVerifiedBy">
  <rdfs:subPropertyOf rdf:resource="#usedBy"/>
  <owl:inverseOf rdf:resource="#verifiesCredential"/>
  <rdfs:domain rdf:resource="#Credential"/>
  <rdfs:range rdf:resource="#SecurityMechanism"/>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A credential is verified by at least one, possibly more countermeasures.</rdfs:comment>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#usedBy">
  <owl:inverseOf rdf:resource="#uses"/>
  <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
  <rdfs:domain>
    <owl:Class>
      <owl:unionOf rdf:parseType="Collection">
        <rdf:Description rdf:about="#Credential"/>
        <rdf:Description rdf:about="#SecurityMechanism"/>
      </owl:unionOf>
    </owl:Class>
  </rdfs:domain>
  <rdfs:range>
    <owl:Class>
      <owl:unionOf rdf:parseType="Collection">
        <rdf:Description rdf:about="#Credential"/>
        <rdf:Description rdf:about="#SecurityMechanism"/>
      </owl:unionOf>
    </owl:Class>
  </rdfs:range>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A countermeasure or credential is used by one or more countermeasures or credentials.</rdfs:comment>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#verifiesCredential">
  <rdfs:subPropertyOf rdf:resource="#uses"/>
  <rdfs:domain rdf:resource="#SecurityMechanism"/>
  <rdfs:range rdf:resource="#Credential"/>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A countermeasure may verify one or more credentials. For example: DigitalSignatureValidation verifies DigitalSignature.</rdfs:comment>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#crosses">
  <owl:inverseOf rdf:resource="#divides"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#divides"/>

<owl:ObjectProperty rdf:about="#dealsWith">
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Esprime la tipologia di dati digitali trattati</rdfs:comment>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#dependsOn">
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Esprime da quali altri reti dipende il funzionamento della rete in oggetto</rdfs:comment>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#employedAt">
  <rdfs:domain rdf:resource="#SecurityMechanism"/>
  <rdfs:range rdf:resource="#AssetLifeCyclePhase"/>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A countermeasure is employed at one or more points in time, e.g. at runtime, at compile time.</rdfs:comment>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#enabledByVulnerability">
  <owl:inverseOf rdf:resource="#enablesThreat"/>
  <rdfs:domain rdf:resource="#Threat"/>
  <rdfs:range rdf:resource="#Vulnerability"/>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A threat is enabled by one or more vulnerabilities.</rdfs:comment>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#enablesThreat">
  <rdfs:domain rdf:resource="#Vulnerability"/>
  <rdfs:range rdf:resource="#Threat"/>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A vulnerability enables one or more threats.</rdfs:comment>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#existsOnAsset">
  <owl:inverseOf rdf:resource="#hasVulnerability"/>
  <rdfs:domain rdf:resource="#Vulnerability"/>
  <rdfs:range rdf:resource="#Asset"/>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A vulnerability exists on one or more assets.</rdfs:comment>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#hasVulnerability">
  <rdfs:domain rdf:resource="#Asset"/>
  <rdfs:range rdf:resource="#Vulnerability"/>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">An asset can have one or more vulnerabilities.</rdfs:comment>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#exploits">
  <owl:inverseOf rdf:resource="#isExploitedBy"/>
  <rdfs:domain rdf:resource="#ATTACK"/>
  <rdfs:range rdf:resource="#Vulnerability"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#isExploitedBy">
  <rdfs:domain rdf:resource="#Vulnerability"/>
  <rdfs:range rdf:resource="#ATTACK"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#goalHasRelatedGoal">
  <owl:inverseOf rdf:resource="#goalHasRelatedGoal"/>
  <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#SymmetricProperty"/>
  <rdfs:domain rdf:resource="#Goal"/>
  <rdfs:range rdf:resource="#Goal"/>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A security goal can have related security goals. For example: confidentiality is related to anonymity, privacy and secrecy.</rdfs:comment>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#hasAsset">
  <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
  <rdfs:domain rdf:resource="#HasAsset"/>
  <rdfs:range rdf:resource="#Asset"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#hasCAPEC">
  <rdfs:domain rdf:resource="#Vulnerability"/>
  <rdfs:range rdf:resource="#CAPEC"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#hasCVE">
  <rdfs:domain rdf:resource="#Vulnerability"/>
  <rdfs:range rdf:resource="#CVE"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#hasCWE">
  <rdfs:domain rdf:resource="#Vulnerability"/>
  <rdfs:range rdf:resource="#CWE"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#hasCountermeasure">
  <owl:inverseOf rdf:resource="#protects"/>
  <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
  <rdfs:domain rdf:resource="#CountermeasureProtectsGoalOfAssetThroughType"/>
  <rdfs:range rdf:resource="#SecurityMechanism"/>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A helper property for the n-ary relations. Denotes a countermeasure.</rdfs:comment>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#protects">
  <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#InverseFunctionalProperty"/>
  <rdfs:domain rdf:resource="#SecurityMechanism"/>
  <rdfs:range rdf:resource="#CountermeasureProtectsGoalOfAssetThroughType"/>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Links a countermeasure to asset, security goal and mechanism type by using the pattern of n-ary relationship as class. This class is represented by CountermeasureProtectsGoalOfAssetThroughMechanismtype.</rdfs:comment>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#hasDestination"/>

<owl:ObjectProperty rdf:about="#hasGoal">
  <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
  <rdfs:domain rdf:resource="#HasGoal"/>
  <rdfs:range rdf:resource="#Goal"/>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A helper property for the n-ary relations. Denotes a goal.</rdfs:comment>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#hasMechanismType">
  <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
  <rdfs:domain rdf:resource="#HasMechanismType"/>
  <rdfs:range rdf:resource="#DefenseStrategy"/>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A helper property for the n-ary relations. Denotes a mechanism type.</rdfs:comment>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#hasMitigations"/>

<owl:ObjectProperty rdf:about="#hasSource"/>

<owl:ObjectProperty rdf:about="#hasSourceAsset">
  <rdfs:domain rdf:resource="#Risk"/>
  <rdfs:range rdf:resource="#Asset"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#hasSourceCAPEC">
  <rdfs:domain rdf:resource="#Risk"/>
  <rdfs:range rdf:resource="#CAPEC"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#hasSourceVuln">
  <rdfs:domain rdf:resource="#Risk"/>
  <rdfs:range rdf:resource="#Vulnerability"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#hasThreat">
  <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
  <rdfs:domain rdf:resource="#HasThreat"/>
  <rdfs:range rdf:resource="#Threat"/>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A helper property for the n-ary relations. Denotes a threat.</rdfs:comment>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#hasUser">
  <rdfs:domain rdf:resource="#SecurityMechanism"/>
  <rdfs:range rdf:resource="#User"/>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A property for determining what kind of users typically interact with a countermeasure.  A useful add-on if one wants to reason about usability requirements of countermeasures.</rdfs:comment>
  <owl:versionInfo rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Not part of the core security ontology.</owl:versionInfo>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#ifSuccessfulLeadsToThreat">
  <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
  <rdfs:domain rdf:resource="#Threat"/>
  <rdfs:range rdf:resource="#Threat"/>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Threats can imply other threats. If one threat has materialised, this may open up for other threats. For example: if the host is compromised a rootkit can be introduced.</rdfs:comment>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#implementsModel">
  <owl:inverseOf rdf:resource="#modelImplementedBy"/>
  <rdfs:domain rdf:resource="#SecurityMechanism"/>
  <rdfs:range rdf:resource="#Model"/>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A countermeasure implements a theoretical security model, e.g. RBAC.</rdfs:comment>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#modelImplementedBy">
  <rdfs:domain rdf:resource="#Model"/>
  <rdfs:range rdf:resource="#SecurityMechanism"/>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A model is implemented by one or more countermeasures.</rdfs:comment>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#isLabelledWithSTRIDE"/>

<owl:ObjectProperty rdf:about="#isProtectedBy"/>

<owl:ObjectProperty rdf:about="#isSuppliedBy">
  <owl:inverseOf rdf:resource="#supply"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#supply"/>

<owl:ObjectProperty rdf:about="#mitigates"/>

<owl:ObjectProperty rdf:about="#protectsFrom"/>

<owl:ObjectProperty rdf:about="#runsOnOperatingSystem">
  <owl:inverseOf rdf:resource="#supportsCountermeasure"/>
  <rdfs:domain rdf:resource="#SecurityMechanism"/>
  <rdfs:range rdf:resource="#OperatingSystem"/>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A countermeasure executes on one or more operating systems. This is for finding countermeasure products for a given operating system. For example: Which firewalls run on Mac OS?</rdfs:comment>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#supportsCountermeasure">
  <rdfs:domain rdf:resource="#OperatingSystem"/>
  <rdfs:range rdf:resource="#SecurityMechanism"/>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">An operating system can support one or more countermeasures. This is for finding countermeasure products for a given operating system. For example: Which firewalls run on Mac OS?</rdfs:comment>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#supportedBy">
  <owl:inverseOf rdf:resource="#supports"/>
  <rdfs:domain rdf:resource="#SecurityMechanism"/>
  <rdfs:range rdf:resource="#SecurityMechanism"/>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A countermeasure is supported (relates to) another countermeasure. Encryption is supported by key management.</rdfs:comment>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#supports">
  <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
  <rdfs:domain rdf:resource="#SecurityMechanism"/>
  <rdfs:range rdf:resource="#SecurityMechanism"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#threatens">
  <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#InverseFunctionalProperty"/>
  <rdfs:domain rdf:resource="#Threat"/>
  <rdfs:range rdf:resource="#ThreatThreatensGoalOfAsset"/>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Links a threat to asset and security goalby using the pattern of n-ary relationship as class. This class is represented by ThreatThreatensGoalOfAsset.</rdfs:comment>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#uses">
  <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
  <rdfs:domain>
    <owl:Class>
      <owl:unionOf rdf:parseType="Collection">
        <rdf:Description rdf:about="#Credential"/>
        <rdf:Description rdf:about="#SecurityMechanism"/>
      </owl:unionOf>
    </owl:Class>
  </rdfs:domain>
  <rdfs:range>
    <owl:Class>
      <owl:unionOf rdf:parseType="Collection">
        <rdf:Description rdf:about="#Credential"/>
        <rdf:Description rdf:about="#SecurityMechanism"/>
      </owl:unionOf>
    </owl:Class>
  </rdfs:range>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A countermeasure or a credential can use one or more countermeasures or credentials.</rdfs:comment>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#usedInAttackTactic">
  <rdfs:domain rdf:resource="#ATTACKtechnique"/>
  <rdfs:range rdf:resource="#ATTACKtactic"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#usedInDefendTactic">
  <rdfs:domain rdf:resource="#DEFENDTechnique"/>
  <rdfs:range rdf:resource="#DEFENDtactic"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#usedInTactic">
