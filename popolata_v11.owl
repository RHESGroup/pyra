  <isLabelledWithSTRIDE rdf:resource="#Tampering"/>
  <consequence rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Authorization::Gain Privileges::</consequence>
  <likelihood rdf:datatype="http://www.w3.org/2001/XMLSchema#string">High</likelihood>
  <severity rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Very High</severity>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Generally these are manually edited files that are not in the preview of the system administrators, any ability on the attackers' behalf to modify these files, for example in a CVS repository, gives unauthorized access directly to the application, the same as authorized users.</rdfs:comment>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Manipulating Writeable Configuration Files</rdfs:comment>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#CAPEC-76">
  <rdf:type rdf:resource="#CAPEC"/>
