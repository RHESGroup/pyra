<owl:DatatypeProperty rdf:about="#deviceType">
  <rdfs:range>
    <rdfs:Datatype>
      <owl:oneOf>
        <rdf:Description>
      <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Database</rdf:first>
        <rdf:rest>
        <rdf:Description>
      <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Firewall</rdf:first>
        <rdf:rest>
        <rdf:Description>
      <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">HSM</rdf:first>
        <rdf:rest>
        <rdf:Description>
      <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">IDS</rdf:first>
        <rdf:rest>
        <rdf:Description>
      <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">IPS</rdf:first>
        <rdf:rest>
        <rdf:Description>
      <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">PC</rdf:first>
