  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Advanced Encryption Standard.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="#BlockCipher">
  <rdfs:subClassOf rdf:resource="#EncryptionAlgorithm"/>
  <rdfs:subClassOf>
    <owl:Restriction>
      <owl:onProperty rdf:resource="#protects"/>
      <owl:someValuesFrom>
        <owl:Class>
          <owl:intersectionOf rdf:parseType="Collection">
            <rdf:Description rdf:about="#_Detection"/>
