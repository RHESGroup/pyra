<owl:DatatypeProperty rdf:about="#ICTAssetType">
  <rdfs:domain rdf:resource="#ICTEntity"/>
  <rdfs:domain>
    <owl:Restriction>
      <owl:onProperty rdf:resource="#composes"/>
      <owl:someValuesFrom rdf:resource="#ICTEntity"/>
    </owl:Restriction>
  </rdfs:domain>
  <rdfs:domain>
    <owl:Restriction>
      <owl:onProperty rdf:resource="#dealsWith"/>
      <owl:someValuesFrom rdf:resource="#Data"/>
    </owl:Restriction>
  </rdfs:domain>
  <rdfs:domain>
    <owl:Restriction>
      <owl:onProperty rdf:resource="#dependsOn"/>
      <owl:someValuesFrom rdf:resource="#ExternalService"/>
    </owl:Restriction>
  </rdfs:domain>
  <rdfs:domain>
    <owl:Restriction>
      <owl:onProperty rdf:resource="#supply"/>
      <owl:someValuesFrom rdf:resource="#ExternalService"/>
    </owl:Restriction>
  </rdfs:domain>
  <rdfs:range>
    <rdfs:Datatype>
      <owl:oneOf>
        <rdf:Description>
      <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">principale</rdf:first>
        <rdf:rest>
        <rdf:Description>
      <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#string">sottobene</rdf:first>
        <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
        </rdf:Description>
        </rdf:rest>
        </rdf:Description>
      </owl:oneOf>
    </rdfs:Datatype>
  </rdfs:range>
</owl:DatatypeProperty>

