  <consequence rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Availability::Unreliable Execution::</consequence>
  <consequence rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Confidentiality::Read Data::</consequence>
