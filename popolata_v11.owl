  <isLabelledWithSTRIDE rdf:resource="#Tampering"/>
  <consequence rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Authorization::Gain Privileges::</consequence>
  <consequence rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Integrity::Modify Data::</consequence>
  <likelihood rdf:datatype="http://www.w3.org/2001/XMLSchema#string">High</likelihood>
  <severity rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Very High</severity>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">An attacker manipulates inputs to the target software which the target software passes to file system calls in the OS. The goal is to gain access to, and perhaps modify, areas of the file system that the target software did not intend to be accessible.</rdfs:comment>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Manipulating Web Input to File System Calls</rdfs:comment>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#CAPEC-77">
  <rdf:type rdf:resource="#CAPEC"/>
