    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/niki/ontologies/2024/11/merged#CAPEC-563 -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/niki/ontologies/2024/11/merged#CAPEC-563">
        <rdf:type rdf:resource="http://www.semanticweb.org/niki/ontologies/2024/11/merged#CAPEC"/>
