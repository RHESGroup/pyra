    <!-- http://www.semanticweb.org/niki/ontologies/2024/11/merged#affects -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/niki/ontologies/2024/11/merged#affects">
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/niki/ontologies/2024/11/merged#isAffectedBy"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/niki/ontologies/2024/11/merged#composedOf -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/niki/ontologies/2024/11/merged#composedOf">
        <rdfs:comment>Esprime quali risorse compongono la rete.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/niki/ontologies/2024/11/merged#composes -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/niki/ontologies/2024/11/merged#composes"/>
    


    <!-- http://www.semanticweb.org/niki/ontologies/2024/11/merged#connects -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/niki/ontologies/2024/11/merged#connects">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#SymmetricProperty"/>
        <rdfs:comment>Esprime le interconnessioni interne ad un nodo o tra più nodi.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/niki/ontologies/2024/11/merged#containsControl -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/niki/ontologies/2024/11/merged#containsControl">
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/niki/ontologies/2024/11/merged#isContainedIn"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/niki/ontologies/2024/11/merged#Product"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/niki/ontologies/2024/11/merged#SecurityMechanism"/>
        <rdfs:comment>A product can contain one or more countermeasure. For example: MS Word contains encryption and access control.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/niki/ontologies/2024/11/merged#containsTechnology -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/niki/ontologies/2024/11/merged#containsTechnology">
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/niki/ontologies/2024/11/merged#residesOn"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/niki/ontologies/2024/11/merged#Technology"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/niki/ontologies/2024/11/merged#Technology"/>
        <rdfs:comment>One technolgoy asset can contain further technology assets. For example: A network contains hosts, hosts contain data.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/niki/ontologies/2024/11/merged#credentialVerifiedBy -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/niki/ontologies/2024/11/merged#credentialVerifiedBy">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/niki/ontologies/2024/11/merged#usedBy"/>
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/niki/ontologies/2024/11/merged#verifiesCredential"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/niki/ontologies/2024/11/merged#Credential"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/niki/ontologies/2024/11/merged#SecurityMechanism"/>
        <rdfs:comment>A credential is verified by at least one, possibly more countermeasures.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/niki/ontologies/2024/11/merged#crosses -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/niki/ontologies/2024/11/merged#crosses">
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/niki/ontologies/2024/11/merged#divides"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/niki/ontologies/2024/11/merged#dealsWith -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/niki/ontologies/2024/11/merged#dealsWith">
        <rdfs:comment>Esprime la tipologia di dati digitali trattati</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/niki/ontologies/2024/11/merged#dependsOn -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/niki/ontologies/2024/11/merged#dependsOn">
        <rdfs:comment>Esprime da quali altri reti dipende il funzionamento della rete in oggetto</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/niki/ontologies/2024/11/merged#divides -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/niki/ontologies/2024/11/merged#divides"/>
    


    <!-- http://www.semanticweb.org/niki/ontologies/2024/11/merged#employedAt -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/niki/ontologies/2024/11/merged#employedAt">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/niki/ontologies/2024/11/merged#SecurityMechanism"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/niki/ontologies/2024/11/merged#AssetLifeCyclePhase"/>
        <rdfs:comment>A countermeasure is employed at one or more points in time, e.g. at runtime, at compile time.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/niki/ontologies/2024/11/merged#enabledByVulnerability -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/niki/ontologies/2024/11/merged#enabledByVulnerability">
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/niki/ontologies/2024/11/merged#enablesThreat"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/niki/ontologies/2024/11/merged#Threat"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/niki/ontologies/2024/11/merged#Vulnerability"/>
        <rdfs:comment>A threat is enabled by one or more vulnerabilities.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/niki/ontologies/2024/11/merged#enablesThreat -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/niki/ontologies/2024/11/merged#enablesThreat">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/niki/ontologies/2024/11/merged#Vulnerability"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/niki/ontologies/2024/11/merged#Threat"/>
        <rdfs:comment>A vulnerability enables one or more threats.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/niki/ontologies/2024/11/merged#existsOnAsset -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/niki/ontologies/2024/11/merged#existsOnAsset">
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/niki/ontologies/2024/11/merged#hasVulnerability"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/niki/ontologies/2024/11/merged#Vulnerability"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/niki/ontologies/2024/11/merged#Asset"/>
        <rdfs:comment>A vulnerability exists on one or more assets.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/niki/ontologies/2024/11/merged#exploits -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/niki/ontologies/2024/11/merged#exploits">
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/niki/ontologies/2024/11/merged#isExploitedBy"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/niki/ontologies/2024/11/merged#ATTACK"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/niki/ontologies/2024/11/merged#Vulnerability"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/niki/ontologies/2024/11/merged#goalHasRelatedGoal -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/niki/ontologies/2024/11/merged#goalHasRelatedGoal">
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/niki/ontologies/2024/11/merged#goalHasRelatedGoal"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#SymmetricProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/niki/ontologies/2024/11/merged#Goal"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/niki/ontologies/2024/11/merged#Goal"/>
        <rdfs:comment>A security goal can have related security goals. For example: confidentiality is related to anonymity, privacy and secrecy.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/niki/ontologies/2024/11/merged#hasAsset -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/niki/ontologies/2024/11/merged#hasAsset">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/niki/ontologies/2024/11/merged#HasAsset"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/niki/ontologies/2024/11/merged#Asset"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/niki/ontologies/2024/11/merged#hasCAPEC -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/niki/ontologies/2024/11/merged#hasCAPEC">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/niki/ontologies/2024/11/merged#Vulnerability"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/niki/ontologies/2024/11/merged#CAPEC"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/niki/ontologies/2024/11/merged#hasCVE -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/niki/ontologies/2024/11/merged#hasCVE">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/niki/ontologies/2024/11/merged#Vulnerability"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/niki/ontologies/2024/11/merged#CVE"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/niki/ontologies/2024/11/merged#hasCWE -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/niki/ontologies/2024/11/merged#hasCWE">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/niki/ontologies/2024/11/merged#Vulnerability"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/niki/ontologies/2024/11/merged#CWE"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/niki/ontologies/2024/11/merged#hasCountermeasure -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/niki/ontologies/2024/11/merged#hasCountermeasure">
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/niki/ontologies/2024/11/merged#protects"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/niki/ontologies/2024/11/merged#CountermeasureProtectsGoalOfAssetThroughType"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/niki/ontologies/2024/11/merged#SecurityMechanism"/>
        <rdfs:comment>A helper property for the n-ary relations. Denotes a countermeasure.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/niki/ontologies/2024/11/merged#hasDestination -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/niki/ontologies/2024/11/merged#hasDestination"/>
    


    <!-- http://www.semanticweb.org/niki/ontologies/2024/11/merged#hasGoal -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/niki/ontologies/2024/11/merged#hasGoal">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/niki/ontologies/2024/11/merged#HasGoal"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/niki/ontologies/2024/11/merged#Goal"/>
        <rdfs:comment>A helper property for the n-ary relations. Denotes a goal.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/niki/ontologies/2024/11/merged#hasMechanismType -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/niki/ontologies/2024/11/merged#hasMechanismType">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/niki/ontologies/2024/11/merged#HasMechanismType"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/niki/ontologies/2024/11/merged#DefenseStrategy"/>
        <rdfs:comment>A helper property for the n-ary relations. Denotes a mechanism type.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/niki/ontologies/2024/11/merged#hasMitigations -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/niki/ontologies/2024/11/merged#hasMitigations"/>
    


    <!-- http://www.semanticweb.org/niki/ontologies/2024/11/merged#hasSource -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/niki/ontologies/2024/11/merged#hasSource"/>
    


    <!-- http://www.semanticweb.org/niki/ontologies/2024/11/merged#hasThreat -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/niki/ontologies/2024/11/merged#hasThreat">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/niki/ontologies/2024/11/merged#HasThreat"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/niki/ontologies/2024/11/merged#Threat"/>
        <rdfs:comment>A helper property for the n-ary relations. Denotes a threat.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/niki/ontologies/2024/11/merged#hasUser -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/niki/ontologies/2024/11/merged#hasUser">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/niki/ontologies/2024/11/merged#SecurityMechanism"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/niki/ontologies/2024/11/merged#User"/>
        <rdfs:comment>A property for determining what kind of users typically interact with a countermeasure.  A useful add-on if one wants to reason about usability requirements of countermeasures.</rdfs:comment>
        <owl:versionInfo>Not part of the core security ontology.</owl:versionInfo>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/niki/ontologies/2024/11/merged#hasVulnerability -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/niki/ontologies/2024/11/merged#hasVulnerability">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/niki/ontologies/2024/11/merged#Asset"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/niki/ontologies/2024/11/merged#Vulnerability"/>
        <rdfs:comment>An asset can have one or more vulnerabilities.</rdfs:comment>
