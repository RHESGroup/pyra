    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/niki/ontologies/2024/11/merged#CAPEC-624 -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/niki/ontologies/2024/11/merged#CAPEC-624">
