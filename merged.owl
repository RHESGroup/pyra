        <consequence>Authorization::Execute Unauthorized Commands::</consequence>
        <likelihood>Medium</likelihood>
        <severity>High</severity>
        <rdfs:comment></rdfs:comment>
        <rdfs:comment>Malicious Logic Insertion into Product Software via Configuration Management Manipulation</rdfs:comment>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/niki/ontologies/2024/11/merged#CAPEC-446 -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/niki/ontologies/2024/11/merged#CAPEC-446">
