        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/niki/ontologies/2024/11/merged#credentialVerifiedBy"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/niki/ontologies/2024/11/merged#SecurityMechanism"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/niki/ontologies/2024/11/merged#credentialVerifiedBy"/>
