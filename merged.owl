    <!-- http://www.semanticweb.org/niki/ontologies/2024/11/merged#AnomalyDetectionSystem -->

    <owl:Class rdf:about="http://www.semanticweb.org/niki/ontologies/2024/11/merged#AnomalyDetectionSystem">
        <owl:equivalentClass rdf:resource="http://www.semanticweb.org/niki/ontologies/2024/11/merged#Behaviour-BasedIntrusionDetectionSystem"/>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/niki/ontologies/2024/11/merged#ByDetectionMethod"/>
