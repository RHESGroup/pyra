    <!-- http://www.semanticweb.org/niki/ontologies/2024/11/merged#ActiveAttack -->

    <owl:Class rdf:about="http://www.semanticweb.org/niki/ontologies/2024/11/merged#ActiveAttack">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/niki/ontologies/2024/11/merged#Threat"/>
