                <owl:someValuesFrom>
                    <owl:Class>
                        <owl:intersectionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="http://www.semanticweb.org/niki/ontologies/2024/11/merged#_DataOnVolatileMedia"/>
                            <rdf:Description rdf:about="http://www.semanticweb.org/niki/ontologies/2024/11/merged#_Integrity"/>
                        </owl:intersectionOf>
                    </owl:Class>
