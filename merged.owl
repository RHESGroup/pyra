            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/niki/ontologies/2024/11/merged#protects"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/niki/ontologies/2024/11/merged#_Technology"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/niki/ontologies/2024/11/merged#protects"/>
                <owl:allValuesFrom>
                    <owl:Class>
                        <owl:intersectionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="http://www.semanticweb.org/niki/ontologies/2024/11/merged#_Technology"/>
                            <owl:Class>
                                <owl:unionOf rdf:parseType="Collection">
                                    <rdf:Description rdf:about="http://www.semanticweb.org/niki/ontologies/2024/11/merged#_Availability"/>
                                    <rdf:Description rdf:about="http://www.semanticweb.org/niki/ontologies/2024/11/merged#_Confidentiality"/>
                                    <rdf:Description rdf:about="http://www.semanticweb.org/niki/ontologies/2024/11/merged#_Integrity"/>
                                </owl:unionOf>
                            </owl:Class>
                            <owl:Class>
                                <owl:unionOf rdf:parseType="Collection">
                                    <rdf:Description rdf:about="http://www.semanticweb.org/niki/ontologies/2024/11/merged#_Deflection"/>
