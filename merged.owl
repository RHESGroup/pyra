    <!-- http://www.semanticweb.org/niki/ontologies/2024/11/merged#ActiveIntrusionDetectionSystem -->

    <owl:Class rdf:about="http://www.semanticweb.org/niki/ontologies/2024/11/merged#ActiveIntrusionDetectionSystem">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/niki/ontologies/2024/11/merged#ByBehaviourOnDetection"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/niki/ontologies/2024/11/merged#protects"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/niki/ontologies/2024/11/merged#_Prevention"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <dc:description>The IDS responds to the suspicious activity by e.g. logging off a user or by reprogramming the firewall to block network traffic from the suspected malicious source. (Webopedia)</dc:description>
        <rdfs:comment>Aka reactive IDS.</rdfs:comment>
    </owl:Class>
    


